@using warpspeed.Helpers;
@using warpspeed.Models;
@model List<string>
@{
    Layout = null;
}
@if(Model.Count()>0){
foreach (string notification in Model)
{
    <div class="p-not">
        <div class="p-not-title">@notification</div>
    </div>
}
}else{
 @Html.Raw("There are no notifcations for this project");   
}
